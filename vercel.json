{
    "headers": [
      {
        "source": "/manifest.json",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Content-Type", "value": "application/json" }
        ]
      },
      {
        "source": "/logo192.png",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Content-Type", "value": "image/png" }
        ]
      },
      {
        "source": "/logo512.png",
        "headers": [
          { "key": "Access-Control-Allow-Origin", "value": "*" },
          { "key": "Content-Type", "value": "image/png" }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://1337brackets-backend.vercel.app/api/$1"
      },
      {
        "source": "/socket.io/(.*)",
        "destination": "https://1337brackets-backend.vercel.app/socket.io/$1"
      }
    ]
  }